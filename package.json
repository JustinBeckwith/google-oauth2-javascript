{
  "name": "@google/oauth2",
  "version": "0.0.1",
  "author": "Google Inc.",
  "description": "A JavaScript Google OAuth2 Client.",
  "engines": {
    "node": ">=4"
  },
  "main": "./build/src/index.js",
  "types": "./build/src/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/google/oauth2-javascript.git"
  },
  "keywords": [
    "google",
    "api",
    "google apis",
    "client",
    "client library"
  ],
  "dependencies": {
    "axios": "^0.18.0",
    "pem-jwk": "^1.5.1"
  },
  "devDependencies": {
    "@types/node": "^9.6.0",
    "@types/sinon": "^4.3.0",
    "@types/source-map-support": "^0.4.0",
    "@types/tape": "^4.2.32",
    "codecov": "^3.0.0",
    "gts": "^0.5.4",
    "js-green-licenses": "^0.5.0",
    "karma": "^2.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-tap": "^4.1.3",
    "karma-webpack": "^3.0.0",
    "nyc": "^11.6.0",
    "opn": "^5.3.0",
    "prettier": "^1.11.1",
    "server-destroy": "^1.0.1",
    "sinon": "^4.0.0",
    "source-map-support": "^0.5.4",
    "tap-spec": "^4.1.1",
    "tape": "^4.9.0",
    "typescript": "~2.7.2",
    "webpack": "^4.2.0",
    "webpack-cli": "^2.0.13"
  },
  "files": [
    "LICENSE",
    "README.md",
    "build/src",
    "package.json"
  ],
  "nyc": {
    "exclude": [
      "build/test"
    ]
  },
  "scripts": {
    "karma": "karma start",
    "testling": "npm run compile && browserify --debug -t brfs build/test/test.oauth2.js | testling -x open",
    "webpack": "webpack",
    "test": "nyc tape --require source-map-support/register build/test/**/*.js | tap-spec",
    "codecov": "nyc report --reporter=json && codecov -f coverage/*.json",
    "clean": "gts clean",
    "prepare": "npm run compile",
    "check": "gts check",
    "compile": "tsc -p .",
    "fix": "gts fix && npm run fix-samples",
    "fix-samples": "prettier --write --single-quote examples/*.js",
    "pretest": "npm run compile",
    "posttest": "npm run check && npm run license-check",
    "license-check": "jsgl --local .",
    "generate-docs": "typedoc --excludePrivate --excludeExternals --mode modules --out docs src && touch docs/.nojekyll",
    "publish-docs": "gh-pages --dotfiles --dist docs --remote upstream"
  },
  "license": "Apache-2.0"
}
